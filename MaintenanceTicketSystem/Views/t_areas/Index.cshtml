@model IEnumerable<MaintenanceTicketSystem.Models.t_areas>

@{
    ViewBag.Title = "Index";
}

<h2>Ubicaciones</h2>

<p>
    @Html.ActionLink("Crear", "Create")
</p>

<table class="table">
    <tr>
        <th>
            Clave
        </th>
        <th>
            Nombre
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.area)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.descripcion)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.area }) |
                @Html.ActionLink("Eliminar", "Delete", new { id = item.area })
            </td>
        </tr>
    }

</table>

